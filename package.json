{
  "name": "denuvo",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build --base /slides/denuvo/ && shx echo \"ErrorDocument 404 /slides/denuvo/404.html\" > ./dist/.htaccess",
    "dev": "slidev --open",
    "export": "slidev export --wait-until none --wait 20000 --dark --with-clicks --format pdf"
  },
  "dependencies": {
    "@slidev/cli": "^52.0.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "playwright-chromium": "^1.54.1",
    "prettier": "^3.6.2",
    "prettier-plugin-slidev": "^1.0.5",
    "shx": "^0.4.0"
  }
}